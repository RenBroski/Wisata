                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="3dp"
                android:text="Login"
                android:textAlignment="center"
                android:textColor="#ffff"

                android:textStyle="bold" />


        </LinearLayout>

